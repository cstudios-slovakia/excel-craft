{
  "name": "cstudios-slovakia/excel-craft",
  "description": "XLSX export for Craft CMS",
  "type": "craft-plugin",
  "keywords": [
    "craft",
    "xlsx"
  ],
  "authors": [
    {
      "name": "Cstudios",
      "homepage": "https://cstudios.sk/"
    }
  ],
  "support": {
    "email": "support@cstudios.sk"
  },
  "require": {
    "phpoffice/phpspreadsheet": "^1.3 || ^5.3"
  },
  "require-dev": {
    "craftcms/cms": "^5.0.0",
    "craftcms/phpstan": "dev-main",
    "craftcms/ecs": "dev-main",
    "craftcms/rector": "dev-main"
  },
  "archive": {
    "exclude": [
      ".github",
      ".gitignore"
    ]
  },
  "autoload": {
    "psr-4": {
      "Cstudios\\ExcelExport\\": "src/"
    }
  },
  "extra": {
    "name": "Excel Export",
    "handle": "excelexport"
  },
  "scripts": {
    "check-cs": "ecs check --ansi",
    "fix-cs": "ecs check --ansi --fix",
    "phpstan": "phpstan --memory-limit=1G"
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  },
  "version": "1.0.1"
}
